<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:fontFamily">@font/muli</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>


    <style name="CalendarView" />
    <style name="CalendarView.DayOfTheMonth" parent="@style/CalendarView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:minHeight">@dimen/minDayCellHeight</item>
    </style>
    <style name="CalendarView.DayOfTheWeek" parent="@style/CalendarView">
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textColor">@color/colorDarkNavy</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/semi_bold</item>
    </style>
    <style name="CalendarView.MonthTitle" parent="@style/CalendarView">
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textColor">@color/colorDarkNavy</item>
        <item name="android:fontFamily">@font/bold</item>
    </style>
    <style name="CardView" parent="@style/Base.CardView" />
    <style name="CardView.Dark" parent="@style/CardView">
        <item name="cardBackgroundColor">@color/cardview_dark_background</item>
    </style>
    <style name="CardView.Light" parent="@style/CardView">
        <item name="cardBackgroundColor">@color/cardview_light_background</item>
    </style>
    <style name="EasyPermissions" parent="@style/Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>
    <style name="EasyPermissions.Transparent" parent="@style/EasyPermissions">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="ExtraTime">
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textColor">@color/colorGreen</item>
        <item name="android:fontFamily">@font/bold</item>
    </style>
    <style name="ExtraTime1">
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textColor">@color/colorRed</item>
